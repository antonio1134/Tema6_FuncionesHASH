public static void Mostrar(TablaHash[] h){
        System.out.println("INVENTARIO\n");
        System.out.println("[CÃ³digo] \t [Nombre] \t\t[Precio]");
        for(int i=0;i<h.length;i++){
            System.out.println("|"+h[i].dato+"| \t\t |"+h[i].Nombre+"| \t\t |"+h[i].precio+"|");
        }
    }
